{
  "name": "ts-express-typeorm",
  "version": "1.7.2",
  "description": "typescript + express.js + typeorm",
  "author": "Sangdon Park",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "prestart": "pnpm run clean && pnpm run build",
    "start": "NODE_ENV=production node --enable-source-maps dist/main.js",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "dev": "pnpm run build && concurrently -p none \"tsc -p tsconfig.build.json -w\" \"nodemon --watch dist --exec 'node --enable-source-maps' dist/main.js | pino-pretty\"",
    "dev:ts": "nodemon --watch src --exec 'ts-node --esm' src/main.ts | pino-pretty",
    "dev:kill": "pgrep -fl $(pwd)/node_modules/.bin | awk '{print $1}' | xargs -r -I{} kill -2 {} && lsof -ti :8080 | xargs -r -I{} kill {}",
    "dev:debug": "pnpm run build && concurrently -p none \"tsc -p tsconfig.build.json -w\" \"nodemon --watch dist --exec 'DEBUG=1 node --enable-source-maps --inspect=9229' dist/main.js | pino-pretty\"",
    "typecheck": "tsc -p tsconfig.build.json --noEmit --incremental false --tsBuildInfoFile null",
    "circular": "pnpm run prestart && madge --circular dist/",
    "lint": "eslint 'src/**/*'",
    "lint:fix": "eslint --fix 'src/**/*'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@godaddy/terminus": "^4.12.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "0.0.48",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.2",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "16.8.1",
    "helmet": "^7.1.0",
    "inversify": "^6.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "pino": "^8.16.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "type-graphql": "2.0.0-beta.3",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "madge": "^6.1.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}
